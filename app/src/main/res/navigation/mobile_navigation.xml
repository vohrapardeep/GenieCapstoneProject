<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_slideshow">


    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.test.ui.Gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/activity_checkout" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.test.ui.Home.HomeFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
    android:id="@+id/nav_home"
    android:name="com.example.test.ui.Services.ServicesFragment"
    android:label="Services"
    tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_quote"
        android:name="com.example.test.ui.Quote.Quote"
        android:label="Get Quote"
        tools:layout="@layout/quote"/>
    <fragment
        android:id="@+id/nav_contactus"
        android:name="com.example.test.ui.ContactUs.ContactUs"
        android:label="Contact"
        tools:layout="@layout/quote"/>

    <fragment
        android:id="@+id/nav_joinus"
        android:name="com.example.test.ui.JoinUs.Jobs"
        android:label="Join Us"
        tools:layout="@layout/joinus"/>

    <activity
        android:id="@+id/feedback"
        android:name="com.example.test.ui.Feedback.Feedback"
        android:label="Feedback"
        tools:layout="@layout/activity_feedback"/>

</navigation>